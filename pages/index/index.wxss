.pyg_swiper {
  width: 100%;
  height: 340rpx;
}
.pyg_swiper swiper-item navigator image {
  height: 100%;
}
.pyg_catitems {
  margin-top: 12rpx;
  margin-bottom: 43rpx;
  display: flex;
}
.pyg_catitems navigator {
  display: flex;
  flex: 1;
  align-items: center;
  justify-content: center;
}
.pyg_catitems navigator image {
  width: 128rpx;
  height: 140rpx;
}
.pyg_floorData .floor_title {
  width: 100%;
  margin-top: 10rpx;
}
.pyg_floorData .floor_title image {
  height: 59rpx;
}
.pyg_floorData .floor_data {
  margin: 0 16rpx;
}
.pyg_floorData .floor_data navigator {
  float: left;
  width: 33.33%;
}
.pyg_floorData .floor_data navigator:nth-child(-n+4) {
  height: 188rpx;
  border-left: 10rpx;
}
.pyg_floorData .floor_data navigator:nth-child(2),
.pyg_floorData .floor_data navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.pyg_floorData .floor_data image {
  height: 100%;
}
